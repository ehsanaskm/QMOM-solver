FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "6";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.227731;

boundaryField
{
    outlet
    {
        type            calculated;
        value           uniform 0.227731;
    }
    inlet
    {
        type            calculated;
        value           uniform 0.227731;
    }
    walls
    {
        type            calculated;
        value           uniform 0.227731;
    }
}


