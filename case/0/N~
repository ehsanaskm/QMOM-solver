FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      N;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 1e-15;

boundaryField
{
    inlet
    {
        type           	fixedValue;
        value           uniform 15286624;
    }
    outlet
    {
       type               zeroGradient;
    }
    walls
    {
      type             zeroGradient;
    }
}

